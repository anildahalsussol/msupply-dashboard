      "rawQuery": true,
      "rawSql": "with records as \n(\n  select json_array_elements(json::json#>'{data,rows}') as data_array \n  from dashboard_report \n  where method_code like 'dashboardSyncSiteStatistics%' \n), \nheaders as \n( \n  select json_array_elements(json::json#>'{data,header}') as export_date \n  from dashboard_report \n  where method_code like 'dashboardSyncSiteStatistics%' \n), result as\n(\n  select data_array ->> 0 as \"Store\", \n    cast(data_array ->> 1 as int) as \"Days since last SYNC\", \n    cast(data_array ->> 2 as int) as \"# of Unfinalised Supplier Invoices\", \n    cast(data_array ->> 3 as int) as \"Days since Stocktake\", \n    cast(data_array ->> 4 as int) as \"Days since Requisition\", \n    (regexp_split_to_array(data_array ->> 5, '/'))[1]::real / (regexp_split_to_array(data_array ->> 5, '/'))[2]::real as \"CIs per day\", \n    --data_array ->> 5 as \"CIs (entry days/working days)\", \n    (select replace(cast(export_date as varchar), '\"', '') from headers limit 1) as \"Export Date\" \n  from records \n) \nselect * from result where \"Store\" in ($Sites)\n",
